{
  "extends": ["next/core-web-vitals", "prettier", "airbnb", "airbnb-typescript", "airbnb/hooks"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "operator-linebreak": "off",
    "react/require-default-props": "off"
  }
}
